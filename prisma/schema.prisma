generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recommendation {
  id        Int      @id @default(autoincrement())
  animeId   Int
  title     String
  imageUrl  String
  mood      String
  note      String?   // <- Ajout ici
  createdAt DateTime @default(now())
}
